.bd-menu__top {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-box-align: center;
  align-items: center;
  backface-visibility: hidden;
  color: rgba(0,0,0,.3);
  display: flex;
  flex-direction: column;
  font-size: 24rpx;
  height: 39px;
  line-height: 34rpx;
  margin-top: -83px;
  padding: 30px 0 13px;
  position: relative;
  text-align: center;
  transition: margin .3s ease;
}

.bd-menu__top:after {
  border-left: 1rpx solid #e5e5e5;
  content: " ";
  height: 100%;
  left: 0;
  position: absolute;
  top: 0;
  width: 1rpx;
  z-index: 2;
}

.bd-menu__top_text {
  padding-bottom: 16rpx;
}

.bd-menu__top_icon {
  animation: topAnimation 2s infinite;
  height: 28rpx;
  width: 44rpx;
}

.bd-menu__top_on {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-box-align: center;
  align-items: center;
  display: flex;
  flex-direction: column;
  margin-top: 0;
}

@-webkit-keyframes topAnimation {
  0% {
      transform: translateY(0);
  }

  50% {
      transform: translateY(20rpx);
  }

  to {
      transform: translateY(0);
  }
}

@keyframes topAnimation {
  0% {
      transform: translateY(0);
  }

  50% {
      transform: translateY(20rpx);
  }

  to {
      transform: translateY(0);
  }
}

.bd-menu__bottom {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-box-align: center;
  align-items: center;
  color: rgba(0,0,0,.3);
  display: flex;
  flex-direction: column;
  font-size: 24rpx;
  line-height: 34rpx;
  padding: 20rpx 0 calc(10rpx + env(safe-area-inset-bottom));
  text-align: center;
}

.bd-menu__bottom_text {
  padding-top: 16rpx;
}

.bd-menu__bottom_icon {
  animation: bottomAnimation 2s infinite;
  height: 28rpx;
  width: 44rpx;
}

@-webkit-keyframes bottomAnimation {
  0% {
      transform: translateY(0);
  }

  50% {
      transform: translateY(-20rpx);
  }

  to {
      transform: translateY(0);
  }
}

@keyframes bottomAnimation {
  0% {
      transform: translateY(0);
  }

  50% {
      transform: translateY(-20rpx);
  }

  to {
      transform: translateY(0);
  }
}

