.payment-halfScreen__mask {
    background-color: rgba(0,0,0,.5);
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 9999999;
}

.payment-halfScreen__main {
    backface-visibility: hidden;
    background-color: #fff;
    border-radius: 24rpx 24rpx 0 0;
    bottom: 0;
    left: 0;
    overflow: hidden;
    position: fixed;
    right: 0;
    transform: translateY(100%);
    transition: all .3s ease;
    z-index: 99999999;
}

.payment-halfScreen__main_on {
    transform: translate(0);
}

.payment-halfScreen__info {
    padding: 32rpx 48rpx 72rpx;
}

.payment-halfScreen__tips {
    color: rgba(0,0,0,.9);
    font-size: 24rpx;
    line-height: 34rpx;
    margin-bottom: 32rpx;
}

.payment-halfScreen__number {
    -webkit-box-align: start;
    align-items: flex-start;
    border-bottom: 1rpx solid rgba(0,0,0,.1);
    display: flex;
    padding-bottom: 14rpx;
}

.payment-halfScreen__unit {
    color: rgba(0,0,0,.9);
    font-family: WeChatSansSS;
    font-size: 60rpx;
    font-weight: 500;
    line-height: 62rpx;
}

.payment-halfScreen__price {
    -webkit-box-flex: 1;
    -webkit-box-align: center;
    align-items: center;
    display: flex;
    flex: 1;
}

.payment-halfScreen__money {
    color: rgba(0,0,0,.9);
    font-family: WeChatSansSS;
    font-size: 60rpx;
    line-height: 62rpx;
}

.payment-halfScreen__placeholder {
    color: rgba(0,0,0,.3);
    font-size: 44rpx;
    line-height: 62rpx;
}

.payment-halfScreen__cursor {
    animation: cursorAni 1s ease-in 0s infinite;
    background-color: #07c160;
    border-radius: 40rpx;
    height: 54rpx;
    margin-right: 1rpx;
    width: 4rpx;
}

.payment-halfScreen__hd {
    padding: 48rpx 32rpx;
    position: relative;
    text-align: center;
}

.payment-halfScreen__close {
    height: 48rpx;
    left: 48rpx;
    margin-top: -24rpx;
    position: absolute;
    top: 50%;
    width: 48rpx;
}

.payment-halfScreen__title {
    color: rgba(0,0,0,.9);
    font-size: 34rpx;
    font-weight: 500;
    line-height: 48rpx;
}

.payment-halfScreen__bd {
    padding: 8rpx 0 0;
}

.payment-halfScreen__ft {
    background: #f6f6f6;
    padding-bottom: calc(14rpx + env(safe-area-inset-bottom));
}

@-webkit-keyframes cursorAni {
    0% {
        opacity: 1;
    }

    50% {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@keyframes cursorAni {
    0% {
        opacity: 1;
    }

    50% {
        opacity: 0;
    }

    to {
        opacity: 1;
    }
}

@font-face {
    font-family:WeChatSansSS;font-weight:500;src:local("WeChatSansSS-Medium"),url("data:application/x-font-truetype;base64,") format("truetype");
}