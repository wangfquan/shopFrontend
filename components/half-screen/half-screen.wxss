button:after {
    border: none;
}

button[disabled] {
    background-color: #e5e5e5;
    color: hsla(0,0%,100%,.89);
}

.mask {
    background-color: rgba(0,0,0,.5);
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 9999999;
}

.DialogPublic,.receiptSheetDialog {
    background-color: #fff;
    overflow: hidden;
    position: fixed;
    z-index: 10000;
}

.receiptSheetDialog {
    backface-visibility: hidden;
    border-radius: 24rpx 24rpx 0 0;
    bottom: 0;
    left: 0;
    max-height: 75%;
    transform: translateY(100%);
    transition: transform .3s;
    transition: transform .3s,-webkit-transform .3s;
    width: 100%;
}

.receiptSheetDialog-on {
    transform: translate(0);
}

.receiptSheetDialog-leave {
    transform: translateY(100%);
}

.receiptSheetDialog-hd {
    padding: 52rpx 48rpx;
    position: relative;
    text-align: center;
}

.receiptSheetDialog-hd_ex {
    padding: 64rpx 48rpx 52rpx;
}

.hd-text__info .receiptSheetDialog-hd__text {
    color: rgba(0,0,0,.9);
    font-size: 30rpx;
    line-height: 42rpx;
}

.hd-text__info .receiptSheetDialog-hd__info {
    color: rgba(0,0,0,.5);
    font-size: 24rpx;
    line-height: 28rpx;
    margin-top: 8rpx;
}

.receiptSheetDialog-hd__btn {
    height: 48rpx;
    left: 48rpx;
    position: absolute;
    top: 50rpx;
    transform: rotate(45deg);
    width: 48rpx;
    z-index: 2;
}

.receiptSheetDialog-hd__btn:after,.receiptSheetDialog-hd__btn:before {
    background-color: #000;
    content: " ";
    left: 50%;
    position: absolute;
    top: 50%;
    transform: translate(-50%,-50%);
}

.receiptSheetDialog-hd__btn:after {
    height: 48rpx;
    width: 2rpx;
}

.receiptSheetDialog-hd__btn:before {
    height: 2rpx;
    width: 48rpx;
}

.receiptSheetDialog-hd__icon {
    height: 48rpx;
    margin-bottom: 32rpx;
    width: 48rpx;
}

.receiptSheetDialog-hd__text {
    color: rgba(0,0,0,.9);
    font-size: 34rpx;
    font-weight: 500;
    line-height: 48rpx;
}

.receiptSheetDialog-bd {
    padding: 8rpx 48rpx 16rpx;
}

.receiptSheetDialog-bd__img {
    width: 654rpx;
}

.receiptSheetDialog-bd__txt {
    color: rgba(0,0,0,.5);
    font-size: 34rpx;
    line-height: 48rpx;
    text-align: center;
}

.receiptSheetDialog-bd__swiper {
    width: 654rpx;
}

.receiptSheetDialog-swiper__img {
    height: 100%;
    width: 654rpx;
}

.DialogPublic-ft {
    -webkit-box-align: center;
    -webkit-box-pack: center;
    align-items: center;
    display: flex;
    justify-content: center;
    padding: 64rpx 48rpx calc(64rpx + env(safe-area-inset-bottom));
}

.DialogPublic-ft__empty {
    padding-bottom: 40rpx;
}

.DialogPublic-ft__empty_ex {
    padding-bottom: 68rpx;
}

.DialogPublic-ft_adaptation {
    padding: 64rpx 64rpx 132rpx;
}

.DialogPublic-btn {
    -webkit-box-flex: 1;
    background-color: rgba(0,0,0,.05);
    border-radius: 8rpx;
    color: #ffc300;
    flex: 1;
    font-size: 34rpx;
    font-weight: 500;
    line-height: 48rpx;
    margin-right: 32rpx;
    max-width: 368rpx;
    padding: 16rpx;
    text-align: center;
}

.DialogPublic-btn:last-child {
    margin: 0 auto;
}

.DialogPublic-btn_hover {
    background-image: linear-gradient(-180deg,rgba(0,0,0,.1),rgba(0,0,0,.1));
}

.DialogPublic-btn_primary {
    background-color: #ffc300;
    color: #fff;
}

.receiptSheetDialog-bd__img__wrap {
    margin: 0 0 40rpx;
}

.receiptSheetDialog-hd__content {
    -webkit-box-pack: center;
    display: -webkit-inline-box;
    display: -webkit-inline-flex;
    display: inline-flex;
    justify-content: center;
    position: relative;
    width: 100%;
}

.receiptSheetDialog-hd__opt {
    height: 48rpx;
    left: 0;
    margin-top: -24rpx;
    position: absolute;
    top: 50%;
    width: 48rpx;
}