page {
  height: 100%;
  min-height: 100%;
}

button:after {
  border: none;
}

@font-face {
  font-family:WeChatSansStd;font-weight:500;src:local("WeChatSansStd-Medium"),url("data:application/x-font-truetype;base64,") format("truetype");
}

.fee-pop__box {
  position: relative;
}

.fee-pop__box .icon-info {
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 32rpx 32rpx;
  height: 32rpx;
  margin-left: 10rpx;
  width: 32rpx;
}

.fee-pop__box .fee-pop {
  bottom: -122rpx;
  left: -30rpx;
  position: absolute;
  right: 0;
  z-index: 99;
}

.fee-pop__box .fee-pop__main {
  -webkit-box-align: center;
  align-items: center;
  background-color: rgba(0,0,0,.8);
  border-radius: 16rpx;
  box-sizing: border-box;
  display: flex;
  margin: 0 auto;
  padding: 30rpx 24rpx 30rpx 32rpx;
  position: relative;
  width: 568rpx;
}

.fee-pop__box .fee-pop__main:after {
  border-color: transparent transparent rgba(0,0,0,.8);
  border-style: solid;
  border-width: 0 15rpx 15rpx;
  content: "";
  height: 0;
  left: 40rpx;
  position: absolute;
  top: -15rpx;
  width: 0;
  z-index: 2;
}

.fee-pop__box .fee-pop__txt {
  -webkit-box-flex: 1;
  color: #fff;
  flex: 1;
  font-size: 28rpx;
  line-height: 42rpx;
  margin-right: 10rpx;
}

.fee-pop__box .fee-pop__btn {
  background-image: url(https://wx.gtimg.com/resource/feuploader/202103/9e10ff50a8121e426ffe0c9c790388ca_80x80.png);
  background-repeat: no-repeat;
  background-size: 100% auto;
  height: 40rpx;
  width: 40rpx;
}

.bd-menu__item {
  -webkit-box-align: start;
  align-items: flex-start;
  box-sizing: border-box;
  display: flex;
  padding: 24rpx;
}

.bd-menu__item_ex {
  -webkit-box-align: center;
  align-items: center;
}

.necessary {
  background: rgba(239,182,0,.1);
}

.bd-menu__item_off .menu-detail__name {
  color: #ccc;
}

.bd-menu__item_off .menu-detail__price_num,.bd-menu__item_off .menu-detail__price_start {
  color: rgba(239,83,80,.3);
}

.bd-menu__item_off .menu-detail__operation_info {
  border: 2rpx solid rgba(239,83,80,.3);
  color: rgba(239,83,80,.3);
}

.bd-menu__item_off .menu-detail__info_item,.bd-menu__item_off .menu-detail__price_old {
  color: #ccc;
}

.bd-menu__item_off .hd-item__tips {
  display: block;
}

.bd-menu__item_off .info-num__add {
  opacity: .3;
}

.bd-menu__item_off .info-num__add:active {
  transform: scale(1);
}

.bd-menu__item_off .info-num__format,.bd-menu__item_off .menu-bd__marketing {
  opacity: .3;
}

.bd-menu__item_off .menu-item__hd:after {
  visibility: visible;
}

.menu-item__hd {
  height: 176rpx;
  margin-right: 24rpx;
  position: relative;
  width: 176rpx;
}

.menu-item__hd:after {
  background-color: hsla(0,0%,100%,.6);
  bottom: 0;
  content: " ";
  left: 0;
  pointer-events: none;
  position: absolute;
  right: 0;
  top: 0;
  visibility: hidden;
  z-index: 8;
}

.hd-item__img {
  border-radius: 8rpx;
  height: 100%;
  width: 100%;
}

.hd-item__tips {
  background-color: rgba(0,0,0,.5);
  border-radius: 0 0 6rpx 6rpx;
  bottom: 0;
  display: none;
  height: 55rpx;
  line-height: 55rpx;
  right: 0;
  width: 100%;
  z-index: 1;
}

.hd-item__tag,.hd-item__tips {
  color: #fff;
  font-size: 22rpx;
  left: 0;
  position: absolute;
  text-align: center;
}

.hd-item__tag {
  background: #efb600;
  border-radius: 8rpx 0 0 0;
  box-sizing: border-box;
  font-family: PingFangSC-Medium;
  height: 36rpx;
  line-height: 36rpx;
  padding: 0 12rpx;
  top: 0;
}

.tag-green {
  background: #00c250;
}

.tag-red {
  background: #f05450;
}

.tag-gray {
  background: #ccc;
}

.menu-item__bd {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  flex: 1;
  flex-direction: column;
  height: 100%;
}

.menu-info__num,.menu-item__bd {
  -webkit-box-flex: 1;
  display: flex;
}

.menu-info__num {
  -webkit-box-pack: end;
  -webkit-box-align: center;
  align-items: center;
  flex: 1;
  justify-content: flex-end;
}

.info-num__minus {
  background: url(https://wx.gtimg.com/resource/feuploader/202105/fe71a51b9f238c9010b0981f43a8119f_58x58.png) no-repeat;
  background-size: 38rpx 38rpx;
  height: 38rpx;
  width: 38rpx;
}

.info-num__minus:active {
  transform: scale(.9);
}

.info-num__mete {
  color: rgba(0,0,0,.9);
  font-size: 28rpx;
  font-weight: 500;
  line-height: 28rpx;
  text-align: center;
  width: 50rpx;
}

.info-num__add {
  background: url(https://wx.gtimg.com/resource/feuploader/202105/9dae801b0d7b53b5bc80f19c44fee5de_58x58.png) no-repeat;
  background-size: 38rpx 38rpx;
  height: 38rpx;
  width: 38rpx;
}

.info-num__add:active {
  transform: scale(.9);
}

.cell-ft__item {
  border-radius: 4rpx;
}

.info-num__format {
  background: url(https://wx.gtimg.com/resource/feuploader/202104/001e7e05589ecf2549fb830cdc3824fa_144x60.png) no-repeat 50%;
  background-size: 96rpx 40rpx;
  height: 40rpx;
  position: relative;
  width: 96rpx;
}

.info-num__format_dotted {
  display: none;
}

.info-num__format_selected .info-num__format_dotted {
  -webkit-box-pack: center;
  -webkit-box-align: center;
  align-items: center;
  background: #efb600;
  border-radius: 50%;
  color: #fff;
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
  display: inline-flex;
  font-size: 20rpx;
  height: 28rpx;
  justify-content: center;
  line-height: 20rpx;
  position: absolute;
  right: -16rpx;
  text-align: center;
  top: -16rpx;
  width: 28rpx;
}

.menu-detail {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  display: flex;
  flex-direction: column;
  margin-top: -8rpx;
  min-height: 176rpx;
  position: relative;
}

.menu-detail,.menu-detail__top {
  -webkit-box-flex: 1;
  flex: 1;
}

.menu-detail__name {
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: rgba(0,0,0,.9);
  display: -webkit-box;
  font-size: 32rpx;
  font-weight: 700;
  line-height: 44rpx;
  margin-bottom: 16rpx;
  max-width: 310rpx;
  overflow: hidden;
}

.menu-detail__info {
  -webkit-box-align: center;
  align-items: center;
  display: flex;
  margin-bottom: 20rpx;
}

.menu-detail__info_item {
  color: rgba(0,0,0,.3);
  font-size: 24rpx;
  line-height: 24rpx;
  padding-right: 24rpx;
  position: relative;
}

.menu-detail__info_item:after {
  background-color: rgba(0,0,0,.3);
  content: " ";
  height: 20rpx;
  position: absolute;
  right: 12rpx;
  top: 2rpx;
  width: 1rpx;
  z-index: 2;
}

.menu-detail__info_item:last-child {
  padding-right: 0;
}

.menu-detail__info_item:last-child:after {
  display: none;
}

.menu-detail__info_item_ex {
  color: #f15451;
}

.menu-detail__price {
  -webkit-box-align: center;
  align-items: center;
  display: flex;
}

.menu-detail__price_num {
  color: #f15451;
  font-family: WeChatSansStd;
  font-size: 32rpx;
  font-weight: 700;
  line-height: 32rpx;
}

.menu-detail__price_old {
  color: rgba(0,0,0,.3);
  font-family: WeChatSansStd;
  font-size: 24rpx;
  font-weight: 700;
  line-height: 24rpx;
  margin-left: 8rpx;
  text-decoration: line-through;
}

.menu-detail__price_start {
  color: #f15451;
  font-size: 24rpx;
  line-height: 24rpx;
  margin-left: 6rpx;
  margin-top: 4rpx;
}

.menu-detail__operation {
  -webkit-box-align: center;
  align-items: center;
  bottom: 0;
  display: flex;
  position: absolute;
  right: 0;
}

.menu-detail__operation_info {
  border: 2rpx solid #f15451;
  border-radius: 4rpx;
  color: #f15451;
  display: inline-block;
  font-size: 20rpx;
  font-weight: 500;
  line-height: 20rpx;
  margin-top: 10rpx;
  padding: 2rpx 4rpx;
  text-align: center;
}

.menu-detail__operation_limit {
  margin-left: 2rpx;
}

.menu-bd__marketing {
  -webkit-box-flex: 1;
  -webkit-box-align: center;
  -webkit-box-pack: end;
  align-items: center;
  display: flex;
  flex: 1;
  justify-content: flex-end;
}

.menu-bd__marketing .info-num__add {
  opacity: .3;
}

.menu-bd__marketing .info-num__add:active {
  transform: scale(1);
}

.hd-item__marketingicon {
  height: 64rpx;
  left: 6rpx;
  position: absolute;
  top: 6rpx;
  width: 64rpx;
  z-index: 2;
}

.menu-marketing__btn {
  background: url(https://gtimg.wechatpay.cn/resource/feuploader/202205/5add88c12980116aef308cbe75a3bf0a_192x80.png) no-repeat 50%;
  background-size: 96rpx 40rpx;
  height: 40rpx;
  width: 96rpx;
}

.menu-marketing__btn_off {
  background: url(https://gtimg.wechatpay.cn/resource/feuploader/202205/4b3500ad996579e8168dd8b152b90e38_192x80.png) no-repeat 50%;
  background-size: 96rpx 40rpx;
  height: 40rpx;
  width: 96rpx;
}