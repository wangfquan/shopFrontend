.circle {
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  background: hsla(0,0%,100%,.95);
  border-radius: 50%;
  bottom: 128rpx;
  box-shadow: 0 0 8px 0 rgba(0,0,0,.1);
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  height: 120rpx;
  padding-top: 20rpx;
  position: fixed;
  right: 48rpx;
  text-align: center;
  width: 120rpx;
  z-index: 9999;
}

.circle .icon image {
  height: 50rpx;
  margin: auto;
  width: 56rpx;
}

.circle .label {
  color: #e8b647;
  font-size: 20rpx;
  letter-spacing: 0;
}

.sandbox {
  background: #1aad19;
  bottom: 10px;
  color: #fff;
  font-size: 10px;
  left: 10px;
  opacity: .7;
  padding: 3px;
  position: fixed;
  z-index: 99999;
}

.navbar {
  left: 0;
  position: fixed;
  right: 0;
  top: 0;
  z-index: 999999;
}

.title-container {
  -webkit-box-align: center;
  align-items: center;
  display: flex;
  height: 44px;
  position: relative;
}

.image {
  background-position: 50%;
  background-repeat: no-repeat;
  background-size: 48rpx 48rpx;
}

.back,.image {
  height: 100%;
  width: 48rpx;
}

.back {
  padding-left: 32rpx;
}

.back-white {
  background-image: url("data:image/png;base64,");
}

.back-black {
  background-image: url("data:image/png;base64,");
}

.home {
  height: 100%;
  padding-left: 32rpx;
  width: 48rpx;
}

.home-white {
  background-image: url("data:image/png;base64,");
}

.home-black {
  background-image: url("data:image/png;base64,");
}

.title {
  font-size: 17px;
  font-weight: 500;
  left: 116px;
  overflow: hidden;
  position: absolute;
  right: 116px;
  text-align: center;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.navbar-white,.title {
  color: #fff;
}

.navbar-black {
  color: #000;
}

.custom-service {
  background-image: url(https://act.weixin.qq.com/static/images/202007/e4d9764d2b5e436ed66d25f4676c3f38_72x72.png);
}

.custom-receipt {
  background-image: url(https://act.weixin.qq.com/static/images/202008/9ea856a932f40d98a4ce61f512ebf017_72x72.png);
}

.custom-sjt {
  background-image: url(https://gtimg.wechatpay.cn/resource/xres/img/202208/81c9b65b5d404226f65b7b83a271265f_72x72.png);
}

.no-button {
  background-color: initial;
  border-radius: 0;
  margin: 0;
  overflow: visible;
}

button:after {
  border: none;
}