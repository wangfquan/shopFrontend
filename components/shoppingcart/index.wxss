page {
    height: 100%;
    min-height: 100%;
}

button:after {
    border: none;
}

@font-face {
    font-family:WeChatSansStd;font-weight:500;src:local("WeChatSansStd-Medium"),url("data:application/x-font-truetype;base64,") format("truetype");
}

.fee-pop__box {
    position: relative;
}

.fee-pop__box .icon-info {
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: 32rpx 32rpx;
    height: 32rpx;
    margin-left: 10rpx;
    width: 32rpx;
}

.fee-pop__box .fee-pop {
    bottom: -122rpx;
    left: -30rpx;
    position: absolute;
    right: 0;
    z-index: 99;
}

.fee-pop__box .fee-pop__main {
    -webkit-box-align: center;
    align-items: center;
    background-color: rgba(0,0,0,.8);
    border-radius: 16rpx;
    box-sizing: border-box;
    display: flex;
    margin: 0 auto;
    padding: 30rpx 24rpx 30rpx 32rpx;
    position: relative;
    width: 568rpx;
}

.fee-pop__box .fee-pop__main:after {
    border-color: transparent transparent rgba(0,0,0,.8);
    border-style: solid;
    border-width: 0 15rpx 15rpx;
    content: "";
    height: 0;
    left: 40rpx;
    position: absolute;
    top: -15rpx;
    width: 0;
    z-index: 2;
}

.fee-pop__box .fee-pop__txt {
    -webkit-box-flex: 1;
    color: #fff;
    flex: 1;
    font-size: 28rpx;
    line-height: 42rpx;
    margin-right: 10rpx;
}

.fee-pop__box .fee-pop__btn {
    background-image: url(https://wx.gtimg.com/resource/feuploader/202103/9e10ff50a8121e426ffe0c9c790388ca_80x80.png);
    background-repeat: no-repeat;
    background-size: 100% auto;
    height: 40rpx;
    width: 40rpx;
}

.mask {
    background-color: rgba(0,0,0,.5);
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 999999;
}

.menu-empty__main {
    backface-visibility: hidden;
    background-color: #efeff4;
    bottom: 0;
    left: 0;
    position: fixed;
    right: 0;
    text-align: center;
    transition: transform .3s;
    transition: transform .3s,-webkit-transform .3s;
    z-index: 9999999;
}

.menu-empty__confirm {
    background-color: #fff;
    color: #ef5350;
    font-size: 36rpx;
    line-height: 50rpx;
    margin-bottom: 12rpx;
    padding: 30rpx;
}

.menu-empty__confirm:active {
    background-color: #e5e5e5;
}

.menu-empty__cancel {
    background-color: #fff;
    color: #000;
    font-size: 36rpx;
    line-height: 50rpx;
    padding: 40rpx 30rpx;
}

.menu-empty__cancel:active {
    background-color: #e5e5e5;
}

.ft-shopClose {
    -webkit-box-align: start;
    align-items: flex-start;
    display: flex;
    padding: 32rpx;
}

.ft-shopClose__hd {
    border-radius: 8rpx;
    height: 76rpx;
    margin-right: 32rpx;
    width: 76rpx;
}

.ft-shopClose__bd {
    -webkit-box-flex: 1;
    flex: 1;
}

.shopClose-bd__title {
    color: rgba(0,0,0,.9);
    font-size: 28rpx;
    font-weight: 500;
    line-height: 28rpx;
}

.shopClose-bd__note {
    color: rgba(0,0,0,.5);
    font-size: 28rpx;
    line-height: 36rpx;
    margin-top: 18rpx;
}

.shopClose-bd__moreTime {
    -webkit-box-align: center;
    align-items: center;
    display: flex;
    margin-top: 18rpx;
}

.shopClose-bd__moreTime_on .shopClose-moreTime__bd {
    max-width: 100%;
    white-space: normal;
}

.shopClose-moreTime__bd {
    color: rgba(0,0,0,.5);
    font-size: 28rpx;
    line-height: 36rpx;
    max-width: 290rpx;
    overflow: hidden;
    white-space: nowrap;
}

.shopClose-moreTime__ft {
    -webkit-box-align: center;
    align-items: center;
    background-color: #fff;
    display: flex;
    margin-left: 6rpx;
}

.shopClose-moreTime__ft:active {
    opacity: .65;
}

.shopClose-moreTime__more {
    color: rgba(0,0,0,.5);
    font-size: 28rpx;
    line-height: 36rpx;
}

.shopClose-moreTime__icon {
    height: 37rpx;
    margin-left: 3rpx;
    width: 37rpx;
}

.mask-takeaway {
    background-color: rgba(0,0,0,.5);
    bottom: 220rpx;
    top: 0;
    z-index: 999999;
}

.index-ft,.mask-takeaway {
    left: 0;
    position: fixed;
    right: 0;
}

.index-ft {
    background-color: #fff;
    bottom: 0;
    padding-bottom: env(safe-area-inset-bottom);
    z-index: 999;
}

.ft-hd {
    position: relative;
}

.ft-hd:before {
    border-bottom: 1rpx solid #eee;
    bottom: 0;
    content: " ";
    height: 1rpx;
    left: 0;
    position: absolute;
    right: 0;
    z-index: 10000000;
}

.sheet-main {
    background-color: #fff;
    border-radius: 15rpx 15rpx 0 0;
    position: relative;
    z-index: 9999999;
}

.sheet-hd {
    -webkit-box-align: end;
    align-items: flex-end;
    display: flex;
    padding: 48rpx 32rpx 12rpx;
}

.sheet-hd__hd {
    -webkit-box-flex: 1;
    color: rgba(0,0,0,.5);
    flex: 1;
    font-weight: 500;
}

.sheet-hd__bd,.sheet-hd__hd {
    font-size: 28rpx;
    line-height: 28rpx;
}

.sheet-hd__bd {
    color: #576b95;
}

.sheet-hd__bd:active {
    opacity: .8;
}

.sheet-bd {
    max-height: 542rpx;
    padding-bottom: 22rpx;
}

.sheet-bd__cell {
    -webkit-box-align: end;
    -webkit-box-pack: justify;
    align-items: flex-end;
    display: flex;
    justify-content: space-between;
    padding: 32rpx;
    position: relative;
}

.sheet-bd__cell:after {
    border-bottom: 1rpx solid #eee;
    bottom: 0;
    content: " ";
    height: 1rpx;
    left: 40rpx;
    position: absolute;
    right: 40rpx;
    z-index: 2;
}

.sheet-bd__cell:last-child:after {
    display: none;
}

.sheet-cell__hd {
    height: 112rpx;
    margin-right: 24rpx;
    position: relative;
    width: 112rpx;
}

.sheet-cell__hd_image {
    border-radius: 8rpx;
    height: 112rpx;
    width: 112rpx;
}

.sheet-cell__hd_marketinngicon {
    height: 42rpx;
    left: 4rpx;
    position: absolute;
    top: 4rpx;
    width: 42rpx;
    z-index: 2;
}

.sheet-bd__cell_main {
    -webkit-box-align: start;
    align-items: flex-start;
    display: flex;
    margin-right: 64rpx;
    min-height: 112rpx;
    position: relative;
}

.sheet-bd__cell_main,.sheet-cell__bd {
    -webkit-box-flex: 1;
    flex: 1;
}

.sheet-cell__bd {
    margin-top: -6rpx;
}

.sheet-cell__name {
    color: rgba(0,0,0,.9);
    font-size: 32rpx;
    font-weight: 500;
    line-height: 44rpx;
    margin-bottom: 10rpx;
}

.sheet-cell__attri {
    color: rgba(0,0,0,.3);
    font-size: 24rpx;
    line-height: 24rpx;
    margin-bottom: 12rpx;
}

.sheet-bd__money {
    -webkit-box-align: center;
    align-items: center;
    display: flex;
}

.bd-money__hd {
    color: #f15451;
    font-family: WeChatSansStd;
    font-size: 28rpx;
    font-weight: 500;
    line-height: 28rpx;
}

.bd-money__discount {
    border: 2rpx solid #ef5350;
    border-radius: 4rpx;
    color: #f15451;
    display: inline-block;
    font-size: 20rpx;
    font-weight: 500;
    line-height: 20rpx;
    margin-top: 10rpx;
    padding: 2rpx 4rpx;
    text-align: center;
}

.bd-money__discount_limit {
    margin-left: 2rpx;
}

.bd-money__bd {
    color: rgba(0,0,0,.3);
    font-family: WeChatSansStd;
    font-size: 24rpx;
    font-weight: 500;
    line-height: 24rpx;
    margin-left: 8rpx;
    text-decoration: line-through;
}

.sheet-cell__ft {
    margin-top: 12rpx;
}

.ft-bd__main,.sheet-cell__ft {
    -webkit-box-align: center;
    align-items: center;
    display: flex;
}

.ft-bd__main {
    padding: 34rpx 32rpx;
    position: relative;
    z-index: 9999999;
}

.index-ft__hd {
    background-image: url(https://act.weixin.qq.com/static/images/202006/5982ec93dbf33b95280787ca5e00a076_500x500.png);
    background-position: 50%;
    background-repeat: no-repeat;
    background-size: 76rpx 76rpx;
    height: 76rpx;
    margin-right: 24rpx;
    position: relative;
    width: 76rpx;
}

.index-ft__hd:active {
    transform: scale(.89);
}

.ft-hd__num {
    background-color: #ef5350;
    border-radius: 30rpx;
    box-sizing: border-box;
    color: #fff;
    font-size: 22rpx;
    font-weight: 500;
    height: 36rpx;
    line-height: 36rpx;
    min-width: 36rpx;
    overflow: hidden;
    padding: 0 10rpx;
    position: absolute;
    right: -12rpx;
    text-align: center;
    top: -12rpx;
    white-space: nowrap;
    z-index: 10;
}

.index-ft__bd {
    -webkit-box-flex: 1;
    flex: 1;
}

.ft-bd__hd {
    color: rgba(0,0,0,.9);
    font-size: 28rpx;
    line-height: 1;
}

.ft-bd__bd {
    -webkit-box-align: center;
    align-items: center;
    display: flex;
    margin-left: -20rpx;
    margin-top: 12rpx;
}

.ft-bd__item {
    color: rgba(0,0,0,.5);
    font-size: 24rpx;
    line-height: 1;
    padding: 0 20rpx 0 16rpx;
    position: relative;
}

.ft-bd__item:after {
    border-left: 2rpx solid rgba(0,0,0,.3);
    content: " ";
    height: 22rpx;
    left: 0;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    width: 2rpx;
    z-index: 2;
}

.ft-bd__item:first-child {
    padding: 0 16rpx 0 20rpx;
}

.ft-bd__item:first-child:after {
    display: none;
}

.ft-bd__money {
    color: rgba(0,0,0,.9);
    font-family: WeChatSansStd;
    font-size: 36rpx;
    line-height: 1;
}

.index-ft__ft {
    background-color: #ef5350;
    border-radius: 8rpx;
    color: #fff;
    font-size: 32rpx;
    font-weight: 500;
    height: 76rpx;
    line-height: 76rpx;
    text-align: center;
    width: 208rpx;
}

.index-ft__ft:active {
    background-color: #d32f2f;
}

.index-ft__disable,.index-ft__disable:active {
    background-color: rgba(234,81,79,.3);
}

.info-num__minus {
    background: url(https://act.weixin.qq.com/static/images/202003/d4b0274dcb034782d47e69b64ecec3d3.png) no-repeat;
    background-size: 48rpx 48rpx;
    height: 48rpx;
    width: 48rpx;
}

.info-num__minus:active {
    transform: scale(.89);
}

.info-num__mete {
    color: rgba(0,0,0,.9);
    font-size: 28rpx;
    font-weight: 500;
    line-height: 28rpx;
    text-align: center;
    width: 64rpx;
}

.info-num__add {
    background: url(https://act.weixin.qq.com/static/images/202008/536d5e91e8debc97cb7f550ea432f49c_72x72.png) no-repeat;
    background-size: 48rpx 48rpx;
    height: 48rpx;
    width: 48rpx;
}

.info-num__add-ext {
    opacity: .3;
}

.info-num__add:active {
    transform: scale(.9);
}

.info-num__add-ext:active {
    transform: scale(1);
}