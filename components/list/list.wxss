.actionsheet-share {
  backface-visibility: hidden;
  background-color: #ededed;
  border-radius: 24rpx 24rpx 0 0;
  bottom: 0;
  left: 0;
  overflow: hidden;
  position: fixed;
  right: 0;
  transform: translateY(100%);
  transition: all .3s ease;
  z-index: 1000000;
}

.actionsheet-share_on {
  transform: translate(0);
}

.actionsheet-share__bd {
  background-color: #fff;
  margin-bottom: 16rpx;
}

.actionsheet-share__cell {
  background-color: #fff;
  border-radius: 0;
  box-sizing: border-box;
  color: #191919;
  font-size: 34rpx;
  height: 112rpx;
  line-height: 112rpx;
  padding: 0;
  position: relative;
  text-align: center;
}

.actionsheet-share__cell:after {
  border-bottom: 1rpx solid rgba(0,0,0,.1);
  bottom: 0;
  content: "";
  height: 1rpx;
  left: 0;
  position: absolute;
  right: 0;
  z-index: 2;
}

.actionsheet-share__cell_hover {
  background-image: linear-gradient(-180deg,rgba(0,0,0,.1),rgba(0,0,0,.1));
}

.actionsheet-share__cell_ex {
  color: #fa5151;
}

.actionsheet-share__cancel {
  background-color: #fff;
  color: rgba(0,0,0,.9);
  font-size: 34rpx;
  line-height: 48rpx;
  padding: 32rpx 32rpx calc(32rpx + env(safe-area-inset-bottom));
  text-align: center;
}

.actionsheet-share__cancel:active {
  background-image: linear-gradient(-180deg,rgba(0,0,0,.1),rgba(0,0,0,.1));
}

.status-text-entry-container {
  color: rgba(0,0,0,.3);
  font-size: 28rpx;
  position: relative;
  text-align: center;
}

.status-text-entry {
  color: #576b95;
  display: inline-block;
  font-size: 28rpx;
  font-weight: 500;
  padding: 8rpx 12rpx;
}

.status-text-entry:active {
