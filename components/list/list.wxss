}

.announcement-loading__icon {
  animation: loading-icon 1s steps(12) infinite;
  background: url(https://act.weixin.qq.com/static/images/201911/27be63fd114da1349c9e71d725b517a2.png) no-repeat;
  background-size: 40rpx 40rpx;
  height: 40rpx;
  margin-right: 8rpx;
  width: 40rpx;
}

.announcement-loading__text {
  color: #7f7f7f;
  font-size: 28rpx;
  line-break: 1;
}

.announcement-finish__text {
  -webkit-tap-highlight-color: rgba(0,0,0,0);
  color: rgba(0,0,0,.5);
  font-size: 14px;
  line-height: 24px;
}

@-webkit-keyframes loading-icon {
  0% {
      transform: rotate(0deg);
  }

  to {
      transform: rotate(1turn);
  }
}

@keyframes loading-icon {
  0% {
      transform: rotate(0deg);
  }

  to {
      transform: rotate(1turn);
  }
}

